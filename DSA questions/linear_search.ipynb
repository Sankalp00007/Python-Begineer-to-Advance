    "# function calling\n",
    "def linear_search(arr,x):\n",
    "    for i in range (len(arr)):\n",
    "        if arr[i] == x:\n",
    "            return i\n",
    "    return -1\n",
    "\n",
    "# Driver code \n",
    "arr=[20,45,27,47,55,67,75,88,90]\n",
    "x=67\n",
    "result= linear_search(arr,x)\n",
    "print(\"searching element is present at index:\",result)"
   ]
