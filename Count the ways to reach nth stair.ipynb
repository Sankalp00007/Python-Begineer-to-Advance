   "source": [
    "def countstairs(n):\n",
    "    if n== 1:\n",
    "        return 1\n",
    "    elif n==2:\n",
    "        return 2\n",
    "    else:\n",
    "        return countstairs(n-1) + countstairs(n-2)\n",
    "\n",
    "\n",
    "#driver code\n",
    "n=4\n",
    "result = countstairs(n)\n",
    "print(\"the stair count is :\",result)\n"
   ]
